<Window x:Name="Main" x:Class="WebUI.WebUIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStyle="None" AllowsTransparency="True" Loaded="Main_Loaded" ShowInTaskbar="False" Closed="Main_Closed">

    <local:WebView x:Name="WebView" DefaultBackgroundColor="Transparent"/>
</Window>
